{
  "name": "vue_startkit",
  "version": "0.2.0",
  "description": "Template for a new Vue for App project.",
  "main": "main.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SGKuksov/vue_startkit.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "css-mqpacker": "^7.0.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.3",
    "postcss-inline-svg": "^3.1.1",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-svgo": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "svg4everybody": "^2.1.9",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "css-mqpacker": {},
      "postcss-object-fit-images": {},
      "postcss-inline-svg": {},
      "postcss-svgo": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
